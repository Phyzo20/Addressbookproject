pipeline {
    agent any
    tools {
    maven 'mymaven'
    }
    stages {
        stage('build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('deploy') {
            steps {
                sshagent(['techports_jenkins']) {
    //scp <src_dir> user@tomcatIP:/tomcat/webapps
    sh "scp -o StrictHostKeyChecking=no target/*.war techports@44.195.93.182:/home/techports/tomcat/webapps"
}
            }
        }
        
    }
}
